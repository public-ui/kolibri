{
	"name": "@{{kebab name}}/components",
	"version": "0.0.0",
	"license": "EUPL-1.2",
	"author": {
		"name": "{{author}}",
		"email": "{{email}}"
	},
	"sideEffects": false,
	"description": "Das Components-Modul beinhaltet alle zur Komponenten-Bibliothek geh√∂renden Web Components.",
	"main": "dist/index.cjs.js",
	"module": "dist/index.js",
	"customElements": "custom-elements.json",
	"es2015": "dist/esm/index.mjs",
	"es2017": "dist/esm/index.mjs",
	"types": "dist/types/index.d.ts",
	"unpkg": "dist/stencil/stencil.js",
	"collection:main": "dist/collection/index.js",
	"collection": "dist/collection/collection-manifest.json",
	"scripts": {
		"build": "npm run test && npm run build:light",
		"build:light": "npm run clear && mkdir doc && node scripts/transform.css.js && cross-env NODE_ENV=production stencil build --docs --prod && node scripts/hashing.js && node scripts/autogen.doc.js && node scripts/cheat-sheet.js && node scripts/vaadin.js && npm run format -- -w",
		"clear": "rimraf dist doc loader www ../adapters/angular/v11/src ../adapters/angular/v12/src ../adapters/angular/v13/src ../adapters/angular/v14/src ../adapters/angular/v15/src ../adapters/angular/v16/src ../adapters/angular/v17/src ../adapters/hydrate/dist ../adapters/react/src ../adapters/solid/src ../adapters/vaadin/*.java ../adapters/vue/src",
		"depcheck": "depcheck --ignore-bin-package --skip-missing --ignores=@types/*,@public-ui/*,@stencil/*,eslint-*,postcss-windicss,tslib,workbox-build",
		"format": "prettier --check src",
		"lighthouse": "lighthouse --chrome-flags=\"--headless\" --output-path lighthouse.report.html --preset=desktop --quiet",
		"lint": "tsc --noemit && eslint src",
		"unused": "ts-prune src -e",
		"start": "cross-env NODE_ENV=development stencil build --dev --serve --watch --no-open",
		"test": "cross-env NODE_ENV=test stencil test --spec --json --outputFile jest-test-results.json",
		"test:watch": "cross-env NODE_ENV=test stencil test --spec --watchAll",
		"prepack": "npm run build",
		"prepare": "npm-run-all prepare:*",
		"prepare:components-assets": "cpy \"node_modules/@public-ui/components/assets/**/*\" src/assets/ --dot",
		"prepare:components-lib": "cpy \"node_modules/@public-ui/components/dist/kolibri/**/*\" src/assets/kolibri/ --dot"
	},
	"dependencies": {
		"adopted-style-sheets": "1.1.2",
		"@public-ui/components": "2.0.7",
		"@public-ui/schema": "2.0.7"
	},
	"devDependencies": {
		"@stencil/angular-output-target": "0.5.0",
		"@stencil/core": "3.0.0",
		"@stencil/postcss": "2.1.0",
		"@stencil/react-output-target": "0.4.0",
		"@stencil/vue-output-target": "0.7.0",
		"@stencil/sass": "2.0.3",
		"@types/color-convert": "2.0.3",
		"@types/jest": "26.0.24",
		"@types/node": "ts4.9",
		"@types/puppeteer": "7.0.4",
		"@types/wcag-contrast": "3.0.0",
		"@typescript-eslint/eslint-plugin": "5.51.0",
		"@typescript-eslint/parser": "5.51.0",
		"color-convert": "2.0.1",
		"color-rgba": "2.4.0",
		"construct-style-sheets-polyfill": "3.1.0",
		"contrast-color": "1.0.1",
		"cpy-cli": "4.2.0",
		"cross-env": "7.0.3",
		"eslint": "8.33.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-html": "8.0.0",
		"eslint-plugin-jsdoc": "48.0.2",
		"eslint-plugin-json": "3.1.0",
		"eslint-plugin-jsx-a11y": "6.8.0",
		"eslint-plugin-react": "7.33.2",
		"jest": "26.6.3",
		"jest-cli": "26.6.3",
		"lighthouse": "11.0.0",
		"mathjs": "11.5.1",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.21",
		"prettier": "2.8.4",
		"puppeteer": "19.6.3",
		"query-selector-all-shadow-root": "0.0.3",
		"query-selector-shadow-root": "0.0.3",
		"rgba-convert": "0.3.0",
		"rimraf": "3.0.2",
		"stencil-awesome-test": "1.0.5",
		"terser": "5.16.3",
		"tslib": "2.5.0",
		"typescript": "4.9.5",
		"wcag-contrast": "3.0.0"
	},
	"files": [
		"dist",
		"custom-elements.json",
		"vscode-custom-data.json"
	]
}
