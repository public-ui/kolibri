{
	"name": "@{{kebab name}}/components",
	"version": "0.0.0",
	"license": "EUPL-1.2",
	"author": {
		"name": "{{author}}",
		"email": "{{email}}"
	},
	"sideEffects": false,
	"description": "Das Components-Modul beinhaltet alle zur Komponenten-Bibliothek geh√∂renden Web Components.",
	"main": "dist/index.cjs.js",
	"module": "dist/index.js",
	"customElements": "custom-elements.json",
	"es2015": "dist/esm/index.mjs",
	"es2017": "dist/esm/index.mjs",
	"types": "dist/types/index.d.ts",
	"unpkg": "dist/stencil/stencil.js",
	"collection:main": "dist/collection/index.js",
	"collection": "dist/collection/collection-manifest.json",
	"scripts": {
		"clean": "lerna exec --stream -- git clean -f -d -X && git clean -f -d -X",
		"build": "npm run test && npm run build:light",
		"build:light": "npm run clear && cross-env NODE_ENV=production stencil build --docs --prod --stats && npm run format -- -w",
		"republish:adapters": "cd ../adapters/angular && npm run republish && cd ../react && npm run republish && cd ../solid && npm run republish && cd ../vue && npm run republish && cd ..",
		"republish:themes": "cd ../themes && npm run republish && cd ..",
		"republish:self": "npm run reinstall:kolibri && npm run build:light && npm run :publish",
		"republish:all": "npm run republish && npm run republish:themes && npm run republish:adapters && d ../storybook && npm run reinstall:kolibri",
		"republish": "cd ../core && npm run republish && cd ../schema && npm run republish && cd ../library && npm run republish:self",
		"clear": "rimraf dist loader www ../adapters/angular/src ../adapters/react/src ../adapters/solid/src ../adapters/svelte/src ../adapters/vue/src",
		"lighthouse": "lighthouse --chrome-flags=\"--headless\" --output-path lighthouse.report.html --preset=desktop --quiet",
		"generate": "stencil generate",
		"lint": "eslint src/**/*{.ts,.tsx}",
		"format": "prettier --check src/**/*",
		"start": "cross-env NODE_ENV=development stencil build --dev --serve --watch",
		"test": "cross-env NODE_ENV=test stencil test --spec --json --outputFile jest-test-results.json",
		"test:watch": "cross-env NODE_ENV=test stencil test --spec --watchAll",
		"prepack": "npm run build && cp package.json package.bak.json && rimraf dist/collection",
		"postpack": "mv package.bak.json package.json",
		"remove:lock": "rimraf package-lock.json",
		"remove": "npm run remove:lock && git clean -f -d -X",
		"reinstall": "npm run remove && npm i",
		"reinstall:stencil": "npm cache clear -f && rimraf node_modules/@stencil &&npm run remove:lock && npm update @stencil/*",
		":install": "pnpm i && pnpm update",
		":postinstall": "npm i puppeteer --no-save || true",
		":publish": "npm run :install && npm publish --registry=http://localhost:4873 || true",
		":republish": "npm run :unpublish && npm run :publish",
		":unpublish": "npm unpublish -f --registry=http://localhost:4873"
	},
	"dependencies": {
		"@public-ui/components": "^1.5.0-rc.0"
	},
	"devDependencies": {
		"@a11y-ui/core": "^1.0.1-rc.62",
		"@public-ui/themes": "^1.5.0-rc.0",
		"@stencil/angular-output-target": "^0.5.0",
		"@stencil/core": "^3.0.0",
		"@stencil/postcss": "^2.1.0",
		"@stencil/react-output-target": "^0.4.0",
		"@stencil/vue-output-target": "^0.7.0",
		"@stencil/sass": "^2.0.3",
		"@types/color-convert": "^2.0.0",
		"@types/jest": "^26.0.24",
		"@types/node": "ts4.9",
		"@types/puppeteer": "^7.0.4",
		"@types/wcag-contrast": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^5.51.0",
		"@typescript-eslint/parser": "^5.51.0",
		"color-convert": "^2.0.1",
		"color-rgba": "^2.4.0",
		"construct-style-sheets-polyfill": "^3.1.0",
		"contrast-color": "^1.0.1",
		"cross-env": "^7.0.3",
		"eslint": "^8.33.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-html": "^7.1.0",
		"eslint-plugin-jsdoc": "^39.8.0",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-react": "^7.32.2",
		"jest": "^26.6.3",
		"jest-cli": "^26.6.3",
		"lighthouse": "^10.0.0",
		"mathjs": "^11.5.1",
		"postcss": "^8.4.21",
		"prettier": "^2.8.4",
		"puppeteer": "^19.6.3",
		"query-selector-all-shadow-root": "^0.0.3",
		"query-selector-shadow-root": "^0.0.3",
		"rgba-convert": "^0.3.0",
		"rimraf": "^4.1.2",
		"stencil-awesome-test": "^1.0.5",
		"terser": "^5.16.3",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"wcag-contrast": "^3.0.0"
	},
	"files": [
		"dist",
		"custom-elements.json",
		"jest-test-results.json",
		"vscode-custom-data.json"
	]
}
