{
	"name": "@{{kebab name}}/storybook",
	"version": "0.0.0",
	"license": "EUPL-1.2",
	"author": {
		"name": "{{author}}",
		"email": "{{email}}"
	},
	"description": "Dokumentation der Komponenten-Bibliothek.",
	"scripts": {
		"clean": "lerna exec --stream -- git clean -f -d -X && git clean -f -d -X",
		"autogen": "node scripts/autogen.md2mdx.js && node scripts/autogen.stories.js",
		"build": "npm run clear && npm run autogen && build-storybook -o dist/",
		"clear": "rimraf dist",
		"zip:prepare": "rimraf kolibri/demo && git clone //anonymisiert kolibri/demo",
		"zip": "sh scripts/public-pack.sh",
		"start": "npm run autogen && start-storybook -p 6006",
		"remove:lock": "rimraf package-lock.json node_modules",
		"remove": "npm run remove:lock && git clean -f -d -X",
		"npm:install": "cross-env CYPRESS_INSTALL_BINARY=0 npm i",
		"postinstall": "npm-run-all postinstall:*",
		"postinstall:components-assets": "cpy \"node_modules/@public-ui/components/assets/**/*\" src/assets --dot",
		"postinstall:themes-assets": "cpy \"node_modules/@public-ui/themes/assets/**/*\" src/assets --dot",
		"reinstall": "npm run remove && npm i",
		"reinstall:kolibri": "npm cache clear -f && rimraf node_modules/@a11y-ui node_modules/@public-ui package-lock.json && npm update @a11y-ui/* @public-ui/*",
		":sync": "rimraf node_modules/@public-ui && npm update @public-ui/*",
		"changelog": "auto-changelog && auto-changelog --output ../../CHANGELOG.json --template json",
		":install": "pnpm i && pnpm update",
		":publish": "npm run :install && npm publish --registry=http://localhost:4873 || true",
		":republish": "npm run :unpublish && npm run :publish",
		":unpublish": "npm unpublish -f --registry=http://localhost:4873"
	},
	"auto-changelog": {
		"commitLimit": false,
		"output": "src/999-changelog.mdx",
		"sortCommits": "sort",
		"template": "scripts/changelog-template.hbs",
		"unreleased": true
	},
	"devDependencies": {
		"@babel/core": "7.20.12",
		"@etchteam/storybook-addon-status": "4.2.2",
		"@geometricpanda/storybook-addon-badges": "0.2.2",
		"@leanup/form": "1.3.43",
		"@leanup/kolibri-components": "1.0.5",
		"@leanup/kolibri-react": "1.0.5",
		"@public-ui/components": "2.0.4",
		"@public-ui/react": "2.0.4",
		"@public-ui/themes": "2.0.4",
		"@storybook/addon-a11y": "6.5.16",
		"@storybook/addon-actions": "6.5.16",
		"@storybook/addon-backgrounds": "6.5.16",
		"@storybook/addon-controls": "6.5.16",
		"@storybook/addon-docs": "6.5.16",
		"@storybook/addon-essentials": "6.5.16",
		"@storybook/addon-jest": "6.5.16",
		"@storybook/addon-links": "6.5.16",
		"@storybook/addon-toolbars": "6.5.16",
		"@storybook/addon-viewport": "6.5.16",
		"@storybook/addons": "6.5.16",
		"@storybook/api": "6.5.16",
		"@storybook/builder-webpack5": "6.5.16",
		"@storybook/components": "6.5.16",
		"@storybook/core-common": "6.5.16",
		"@storybook/manager-webpack5": "6.5.16",
		"@storybook/node-logger": "6.5.16",
		"@storybook/react": "6.5.16",
		"@storybook/theming": "6.5.16",
		"@types/mermaid": "9.2.0",
		"@types/react": "17.0.53",
		"@types/react-dom": "17.0.18",
		"@typescript-eslint/eslint-plugin": "5.51.0",
		"@typescript-eslint/parser": "5.51.0",
		"auto-changelog": "2.4.0",
		"babel-loader": "9.1.2",
		"cpy-cli": "5.0.0",
		"cross-env": "7.0.3",
		"esbuild-loader": "3.0.0",
		"eslint": "8.33.0",
		"eslint-plugin-json": "3.1.0",
		"eslint-plugin-storybook": "0.6.10",
		"esm": "3.2.25",
		"lit-html": "2.6.1",
		"mermaid": "9.3.0",
