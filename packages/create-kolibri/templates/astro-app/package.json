{
	"name": "{{kebab name}}",
	"version": "0.0.0",
	"description": "{{description}}",
	"author": {
		"name": "{{author}}",
		"email": "{{email}}"
	},
	"contributors": [
		{
			"name": "{{author}}",
			"email": "{{email}}"
		}
	],
	"license": "UNLICENSED",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"check": "astro check && tsc",
		"build": "astro build",
		"preview": "astro preview",
		"astro": "astro",
		"coverage": "cross-env NODE_ENV=test nyc mocha",
		"cucumber": "cross-env NIGHTWATCH_ENV=chrome cucumber-js tests/cucumber/features/**/*.feature --require @babel/register --require cucumber.conf.js --require tests/cucumber/step-definitions --format node_modules/cucumber-pretty --format json:.reports/cucumber/report.json",
		"e2e": "nightwatch",
		"-format": "prettier --check {src,tests}/**",
		"-lint": "eslint \"{src,tests}/**/*.{html,js,json,jsx,ts,tsx,gql,graphql}\"",
		"test": "cross-env NODE_ENV=test mocha",
		"update": "ncu -t minor -u && ncu",
		"unused": "ts-prune -e src"
	},
	"dependencies": {
		"@astrojs/react": "1.2.2",
		"@astrojs/preact": "1.2.0",
		"@leanup/stack": "1.3.39",
		"@playwright/test": "1.28.0",
		"@public-ui/components": "1.1.13-rc.1",
		"@public-ui/core": "1.1.13-rc.2",
		"@public-ui/react": "1.1.13-rc.1",
		"@public-ui/themes": "1.1.13-rc.1",
		"@types/react": "18.0.25",
		"@types/node": "18.11.9",
		"@types/react-dom": "18.0.9",
		"astro": "1.6.10",
		"chromedriver": "107.0.3",
		"nightwatch-axe-verbose": "2.0.3",
		"npm-check-updates": "16.4.1",
		"playwright": "1.28.0",
		"preact": "10.11.3",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"rimraf": "3.0.2",
		"ts-prune": "0.10.3"
	}
}
