{
	"name": "{{kebab name}}",
	"version": "0.0.0",
	"description": "{{description}}",
	"license": "UNLICENSED",
	"type": "module",
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"check": "astro check && tsc",
		"build": "astro build",
		"preview": "astro preview",
		"astro": "astro",
		"coverage": "cross-env NODE_ENV=test nyc mocha --exit",
		"cucumber": "cross-env NIGHTWATCH_ENV=chrome cucumber-js tests/cucumber/features/**/*.feature --require @babel/register --require cucumber.conf.js --require tests/cucumber/step-definitions --format node_modules/cucumber-pretty --format json:.reports/cucumber/report.json",
		"e2e": "nightwatch",
		"-format": "prettier --check {src,tests}/**",
		"-lint": "eslint \"{src,tests}/**/*.{html,js,json,jsx,ts,tsx,gql,graphql}\"",
		"prepare": "npm-run-all prepare:*",
		"prepare:components-assets": "cpy \"node_modules/@public-ui/components/assets/**/*\" src/assets --dot",
		"prepare:themes-assets": "cpy \"node_modules/@public-ui/themes/assets/**/*\" src/assets --dot",
		"test": "cross-env NODE_ENV=test mocha --exit",
		"unused": "ts-prune -e src"
	},
	"devDependencies": {
		"@astrojs/react": "1.2.2",
		"@astrojs/preact": "1.2.0",
		"@leanup/stack": "1.3.39",
		"@public-ui/components": "2.0.7",
		"@public-ui/react": "2.0.7",
		"@public-ui/themes": "2.0.7",
		"@types/react": "18.0.26",
		"@types/node": "18.11.17",
		"@types/react-dom": "18.0.9",
		"astro": "1.7.2",
		"chromedriver": "121.0.0",
		"cpy-cli": "5.0.0",
		"nightwatch-axe-verbose": "2.0.7",
		"npm-check-updates": "16.6.0",
		"npm-run-all": "4.1.5",
		"preact": "10.11.3",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"rimraf": "3.0.2",
		"ts-prune": "0.10.3"
	},
	"files": [
		"dist"
	]
}
