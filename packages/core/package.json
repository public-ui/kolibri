		"component framework",
		"component library",
		"design system",
		"style guide",
		"ui framework",
		"ui library",
		"web components"
	],
	"scripts": {
		"build": "unbuild",
		"eslint": "eslint src",
		"tsc": "tsc --noemit",
		"lint": "npm run eslint && npm run tsc",
		"prepack": "unbuild",
		":install": "pnpm i && pnpm update",
		":publish": "npm run :install && npm publish --registry=http://localhost:4873 || true",
		":republish": "npm run :unpublish && npm run :publish",
		":unpublish": "npm unpublish -f --registry=http://localhost:4873"
	},
	"dependencies": {
		"@a11y-ui/core": "1.0.1-rc.6",
		"rxjs": "7.6.0"
	},
	"devDependencies": {
		"@types/node": "18.11.10",
		"@typescript-eslint/eslint-plugin": "5.45.0",
		"@typescript-eslint/parser": "5.45.1",
		"eslint": "8.29.0",
		"eslint-plugin-no-loops": "0.3.0",
		"prettier": "2.8.0",
		"typescript": "4.9.3",
		"unbuild": "1.0.1"
	},
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	]
