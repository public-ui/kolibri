	"description": "The generic types and core logic for the KoliBri (@public-ui) accessible web component library.",
	"keywords": [
		"accessibility",
		"accessible",
		"bitv",
		"framework",
		"library",
		"designsystem",
		"design",
		"system",
		"web components",
		"webcomponents",
		"aria",
		"wai",
		"axe",
		"custom elements",
		"styleguide",
		"style",
		"guide",
		"ui",
		"html",
		"css",
		"web",
		"a11y",
		"w3c",
		"webstandard",
		"wcag"
	],
	"scripts": {
		"build": "unbuild",
		"eslint": "eslint src",
		"tsc": "tsc --noemit",
		"format": "prettier -c src",
		"lint": "npm run eslint && npm run tsc",
		"ncu:major": "ncu",
		"ncu:minor": "ncu -t minor -u",
		"ncu:patch": "ncu -t patch -u",
		"prepack": "unbuild",
		"update": "pnpm ncu:patch && pnpm ncu:minor && pnpm ncu:major"
	},
	"dependencies": {
		"@a11y-ui/core": "1.0.1-rc.56",
		"i18next": "22.4.9"
	},
	"devDependencies": {
		"@commitlint/cli": "17.4.2",
		"@commitlint/config-conventional": "17.4.2",
		"@types/node": "18.11.18",
		"@typescript-eslint/eslint-plugin": "5.50.0",
		"@typescript-eslint/parser": "5.49.0",
		"eslint": "8.33.0",
		"eslint-plugin-no-loops": "0.3.0",
		"npm-check-updates": "16.6.3",
		"prettier": "2.8.3",
		"ts-prune": "0.10.3",
		"typescript": "4.9.5",
		"unbuild": "1.1.1"
	},
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	]
}
