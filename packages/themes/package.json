	"description": "Das Themes-Modul beinhaltet zahlreiche Themes (Styleguides) zur Komponenten-Bibliothek.",
	"scripts": {
		"build": "unbuild",
		"prepack": "unbuild",
		":install": "pnpm i && pnpm update",
		":publish": "npm run :install && npm publish --registry=http://localhost:4873 || true",
		":republish": "npm run :unpublish && npm run :publish",
		":unpublish": "npm unpublish -f --registry=http://localhost:4873"
	},
	"dependencies": {
		"@public-ui/schema": "1.1.13-rc.2"
	},
	"devDependencies": {
		"@types/node": "18.11.9",
		"typescript": "4.9.3",
		"unbuild": "1.0.1"
	},
	"peerDependencies": {
		"@public-ui/components": "1.1.13-rc.1"
	},
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"assets",
		"reports",
		"dist"
	]
}
