{
	"name": "@public-ui/components",
	"version": "1.2.0-rc.1",
	"license": "EUPL-1.2",
	"homepage": "https://public-ui.github.io",
	"repository": "https://github.com/public-ui/kolibri",
	"bugs": {
		"url": "https://github.com/public-ui/kolibri/issues",
		"email": "kolibri@itzbund.de"
	},
	"author": {
		"name": "Informationstechnikzentrum Bund",
		"email": "kolibri@itzbund.de"
	},
	"main": "dist/index.cjs.js",
	"module": "dist/index.js",
	"customElements": "custom-elements.json",
	"es2015": "dist/esm/index.mjs",
	"es2017": "dist/esm/index.mjs",
	"types": "dist/types/index.d.ts",
	"unpkg": "dist/stencil/stencil.js",
	"collection:main": "dist/collection/index.js",
	"collection": "dist/collection/collection-manifest.json",
	"sideEffects": false,
	"description": "Contains all web components that belong to KoliBri - The accessible HTML-Standard.",
	"keywords": [
		"accessibility",
		"accessible",
		"bitv",
		"framework",
		"library",
		"designsystem",
		"design",
		"system",
		"web components",
		"webcomponents",
		"aria",
		"wai",
		"axe",
		"custom elements",
		"styleguide",
		"style",
		"guide",
		"ui",
		"html",
		"css",
		"web",
		"a11y",
		"w3c",
		"webstandard",
		"wcag"
	],
	"scripts": {
		"build": "npm run test && npm run build:light && node scripts/cheat-sheet.js && npm run format -- -w",
		"build:light": "npm run clear && mkdir doc && cross-env NODE_ENV=production stencil build --docs --prod --stats && node scripts/hashing.js && node scripts/autogen.doc.js",
		"clear": "rimraf dist doc loader www ../adapters/angular/src ../adapters/hydrate/dist ../adapters/react/src ../adapters/solid/src ../adapters/svelte/src ../adapters/vue/src",
		"depcheck": "depcheck --ignore-bin-package --skip-missing --ignores=@types/*,@public-ui/*,@stencil/*,eslint-*,postcss-windicss,tslib,workbox-build",
		"format": "prettier --check src",
		"lighthouse": "lighthouse --chrome-flags=\"--headless\" --output-path lighthouse.report.html --preset=desktop --quiet",
		"lint": "eslint src && tsc --noemit",
		"start": "cross-env NODE_ENV=development stencil build --dev --serve --watch",
		"test": "cross-env NODE_ENV=test stencil test --spec --json --outputFile jest-test-results.json",
		"test:watch": "cross-env NODE_ENV=test stencil test --spec --watchAll",
		"prepack": "npm run build && cp package.json package.bak.json && rimraf dist/collection dist/kolibri/assets/@leanup dist/types/assets/@leanup && node scripts/anonymous.js && node scripts/minify.js",
		"postpack": "mv package.bak.json package.json"
	},
	"dependencies": {
		"@public-ui/schema": "1.2.0-rc.2"
	},
	"devDependencies": {
		"@public-ui/core": "1.2.0",
		"@stencil/angular-output-target": "0.4.0",
		"@stencil/core": "2.21.0",
		"@stencil/postcss": "2.1.0",
		"@stencil/react-output-target": "0.3.1",
		"@stencil/sass": "2.0.2",
		"@stencil/solid-output-target": "file:node_martin/stencil-solid-output-target-0.0.1.tgz",
		"@stencil/svelte-output-target": "0.0.3",
		"@stencil/vue-output-target": "0.6.2",
		"@types/color-convert": "2.0.0",
		"@types/jest": "26.0.24",
		"@types/node": "18.11.18",
		"@types/wcag-contrast": "3.0.0",
		"@typescript-eslint/eslint-plugin": "5.48.1",
		"@typescript-eslint/parser": "5.48.1",
		"color-convert": "2.0.1",
		"color-rgba": "2.4.0",
		"cross-env": "7.0.3",
		"eslint": "8.32.0",
		"eslint-config-prettier": "8.6.0",
		"eslint-plugin-html": "7.1.0",
		"eslint-plugin-jsdoc": "39.6.4",
		"eslint-plugin-json": "3.1.0",
		"eslint-plugin-jsx-a11y": "6.7.1",
		"eslint-plugin-no-loops": "0.3.0",
		"eslint-plugin-react": "7.32.0",
		"jest": "26.6.3",
		"jest-cli": "26.6.3",
		"lighthouse": "9.6.8",
		"mathjs": "11.5.0",
		"postcss": "8.4.21",
		"postcss-windicss": "1.0.0",
		"prettier": "2.8.3",
		"query-selector-all-shadow-root": "0.0.3",
		"query-selector-shadow-root": "0.0.3",
		"rgba-convert": "0.3.0",
		"rimraf": "4.0.7",
		"stencil-awesome-test": "1.0.4",
		"terser": "5.16.1",
		"tslib": "2.4.1",
		"typescript": "4.9.4",
		"wcag-contrast": "3.0.0",
		"workbox-build": "6.5.4"
	},
	"files": [
		"dist/",
		"doc/",
		"cheat-sheet.html",
		"custom-elements.json",
		"jest-test-results.json",
		"vscode-custom-data.json"
	]
}
