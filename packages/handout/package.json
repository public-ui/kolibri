{
	"name": "@public-ui/handout",
	"version": "1.5.0-rc.12",
	"description": "This package contains an overview of all components per theme as a handout.",
	"license": "EUPL-1.2",
	"homepage": "https://public-ui.github.io",
	"repository": "https://github.com/public-ui/kolibri",
	"bugs": {
		"url": "https://github.com/public-ui/kolibri/issues",
		"email": "kolibri@itzbund.de"
	},
	"author": {
		"name": "Informationstechnikzentrum Bund",
		"email": "kolibri@itzbund.de"
	},
	"scripts": {
		"clean": "git clean -f -d -X",
		"build": "rimraf dist && cross-env NODE_ENV=production webpack",
		"format": "prettier --check {src,tests}/**",
		"lint": "eslint \"{src,tests}/**/*.{html,js,json,jsx,ts,tsx,gql,graphql}\"",
		"serve": "cross-env NODE_ENV=development webpack serve --devtool=source-map --no-hot",
		"start": "npm run serve -- --open",
		"update": "ncu -t minor -u && ncu",
		"unused": "ts-prune -e src"
	},
	"dependencies": {
		"@public-ui/components": "1.5.0-rc.12",
		"@public-ui/react": "1.5.0-rc.12",
		"@public-ui/themes": "1.5.0-rc.12",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@leanup/stack": "^1.3.43",
		"@leanup/stack-react": "^1.3.43",
		"@leanup/stack-webpack": "^1.3.43",
		"@types/react": "^18.0.33",
		"@types/react-dom": "^18.0.11",
		"@unocss/preset-mini": "^0.47.6",
		"@unocss/webpack": "^0.47.6",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-react": "^7.32.2",
		"npm-check-updates": "^16.10.7",
		"rimraf": "^3.0.2",
		"ts-prune": "^0.10.3",
		"typescript": "^4.9.5"
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	}
}
